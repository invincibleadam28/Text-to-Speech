   "source": [
    "from gtts import gTTS\n",
    "import os\n",
    "\n",
    "f = open('text.txt')\n",
    "\n",
    "t = f.read()\n",
    "language = 'en'\n",
    "\n",
    "audio = gTTS(text = t, lang= language, slow = False)\n",
    "audio.save('text_to_speech.wav')\n",
    "os.system('text_to_speech.wav')"
   ]
